#!/usr/bin/env node
'use strict';

/**
 * Module Dependencies
 */
var cli = require('../lib/cli'),
    argv = require('optimist')
            .options(
              'v', {
                alias: 'verbose'
              },
              'q', {
                alias: 'quiet'
              },
              'h', {
                alias: 'help'
              }
            )
            .argv;

// shift off node and script name
//args.shift(); args.shift();

// run the command line client
//console.log(argv);
cli.argv(argv);